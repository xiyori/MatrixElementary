<Window x:Name="latexWindow" x:Class="Matrix_Elementary.LaTeX_ExportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Matrix_Elementary"
        mc:Ignorable="d"
        Title="LaTeX Export" Height="472.76" Width="465.776" Loaded="Window_Loaded" PreviewKeyUp="latexWindow_PreviewKeyUp" Icon="images/ico_white.ico">
    <Window.Resources>
        <Style TargetType="TextBox" x:Name="DefaultTextBox"/>
    </Window.Resources>
    <Grid>
        <TextBox x:Name="textBox1" ScrollViewer.HorizontalScrollBarVisibility="Auto"
         ScrollViewer.VerticalScrollBarVisibility="Auto" HorizontalAlignment="Left" Height="419" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="435" FontSize="16" AcceptsReturn="True"/>
        <local:FadingPopupControl x:Name="popup1"/>
    </Grid>
</Window>
